    "extract-text-webpack-plugin": "1.0.1",
    "gulp.spritesmith": "^6.2.1",
    "jquery": "2.2.1",
    "lodash": "4.8.2",
    "particles.js": "2.0.0",
    "path": "0.12.7",
    "react": "0.14.7",
    "react-color": "2.0.0",
    "react-dom": "0.14.7",
    "react-router": "2.0.0",
    "adm-zip": "^0.4.7",
    "bootstrap": "3.3.7",
    "archiver": "^1.1.0"
  }
