{
  "name": "web-painter",
  "version": "0.0.1",
  "description": "This is a WYSIWYG web page editor built by Electron.",
  "main": "main.js",
  "scripts": {
    "dev-app": "electron ./main.js DEV",
    "dev-file": "webpack-dev-server --progress --colors --display-error-details --inline --hot --content-base root/build --config webpack/webpack.dev.config.js",
    "deploy-app": "electron ./main.js DEPLOY",
    "deploy-file": "NODE_ENV=production webpack -p --display-error-details --config webpack/webpack.deploy.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sekaiamber/web-painter.git"
  },
  "keywords": [
    "Electron"
  ],
  "author": "sekaiamber",
  "license": "MIT and GPL (version 2 or later) Licenses",
  "bugs": {
    "url": "https://github.com/sekaiamber/web-painter/issues"
  },
  "homepage": "https://github.com/sekaiamber/web-painter#readme",
  "devDependencies": {
    "electron-prebuilt": "^1.2.0",
    "autoprefixer-loader": "3.2.0",
    "babel-core": "6.6.0",
    "babel-jest": "11.0.2",
    "babel-loader": "6.2.4",
    "babel-plugin-react-transform": "2.0.2",
    "babel-polyfill": "6.7.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "css-loader": "0.23.1",
    "file-loader": "0.8.5",
    "gulp": "^3.9.1",
    "html-webpack-plugin": "2.12.0",
    "img-loader": "1.2.2",
    "jest-cli": "^11.0.2",
    "jshint": "2.9.1",
    "jshint-loader": "0.8.3",
    "node-sass": "3.4.2",
    "react-transform-catch-errors": "1.0.2",
    "react-transform-hmr": "1.0.4",
    "redbox-react": "1.2.2",
    "sass-loader": "3.1.2",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "webpack": "1.12.14",
    "webpack-dev-server": "1.14.1"
  },
  "dependencies": {
    "antd": "^1.8.0",
    "classnames": "2.2.3",
    "clipboard": "^1.5.10",
    "echarts": "3.1.3",
    "extract-text-webpack-plugin": "1.0.1",
    "gulp.spritesmith": "^6.2.1",
    "jquery": "2.2.1",
    "lodash": "4.8.2",
    "particles.js": "2.0.0",
    "path": "0.12.7",
    "react": "0.14.7",
    "react-color": "2.0.0",
    "react-dom": "0.14.7",
    "react-router": "2.0.0"
  }
}
